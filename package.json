{
  "name": "edu_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "concurrently": "^8.0.1",
    "cookie-session": "^2.0.0",
    "express": "^4.18.2",
    "json-server": "^0.17.2",
    "mongodb": "^5.2.0",
    "mongoose": "^7.0.4",
    "parcel": "^2.8.3",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.3.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start ",
    "build": "parcel src/index.html",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\" --coverage --watchAll",
    "lint": "eslint \"**/*.{js,jsx}\" --ignore-pattern *.test.js --ignore-pattern *.test.jsx",
    "uitest": "jest --coverage",
    "eject": "react-scripts eject",
    "server": "node backend/server.js",
    "json-server": "json-server --watch frontend/src/json-server/db.json",
    "testbackend": "jest backend/test --coverage",
    "dev": "concurrently \"npm run server\" \"npm run testbackend\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "buffer": "^5.7.1",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^27.5.1",
    "process": "^0.11.10"
  }
